%Z9  Расширение выходного номера в тексте    03.07.95

- рп  = -рп 
- рв  = -рв 
- рз  = -рз 
- р   = -р 
- ргс = -ргс 
- фз  = -фз 
- I   = -I  
- 1   = -1
- к   = -к  
- гд  = -гд 
- сф  = -сф 
- фкз = -фкз 
- ФКЗ = -фкз 
  сф  = _сф  
  гд  = _гд  

